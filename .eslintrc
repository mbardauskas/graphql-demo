{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": "wix/react-native",
  "plugins": [
    "import"
  ],
  "rules": {
    "prefer-arrow-callback": 0,
    "react/prop-types": 0,
    "no-console": ["error", {"allow": ["warn", "error"]}],
    "max-params": 0,
    "prefer-template": 0,
    "react/jsx-no-bind": 0,
    "no-duplicate-imports": 0,
    "import/no-duplicates": 2
  },
  "globals": {
    "$Shape": false
  }
}
